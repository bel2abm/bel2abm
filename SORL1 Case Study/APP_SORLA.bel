# Statements Section

SET STATEMENT_GROUP = "SORLA in APP processing"

SET Citation = {"PubMed", "EMBO J. 2012 Jan 4;31(1):187-200. doi: 10.1038/emboj.2011.352. Epub 2011 Oct 11.", "21989385"}

SET Evidence = " (i) APP as well as a- and b-secretases exist in an equilibrium of monomeric and homodimeric forms; 
(ii) SORLA can reversibly form a complex with monomeric APP to reduce the formation of substrate oligomers (Figure 10A–C) 
and to impair processing efﬁciency; and (iii) SORLA does not directly impact on enzymatic activity of secretases "

# the statement proteinAbundance(HUPSON:"bodily-process output protein") makes the subject of the statement an agent type that
# is introduced as a fresh agent with stochastic pulses
proteinAbundance(MSO:"Alpha secretase") isA proteinAbundance(HUPSON:"bodily-process output protein")
proteinAbundance(MSO:"Beta secretase") isA proteinAbundance(HUPSON:"bodily-process output protein")
p(HGNC:APP) isA proteinAbundance(HUPSON:"bodily-process output protein")
p(HGNC:SORL1) isA proteinAbundance(HUPSON:"bodily-process output protein")

complex(p(HGNC:APP), proteinAbundance(MSO:"Alpha secretase"))
complex(p(HGNC:APP), proteinAbundance(MSO:"Beta secretase"))

reaction(
   reactants (
     complex(p(HGNC:APP), proteinAbundance(MSO:"Alpha secretase"))
      ) ,
   products (
      proteinAbundance(MSO:"Alpha secretase"),
      p("sappalpha"),
      p("C83")
     )     )
     
reaction(
   reactants (
     complex(p(HGNC:APP), proteinAbundance(MSO:"Beta secretase")) 
     ),
   products (
        proteinAbundance(MSO:"Beta secretase"),
        p("sappbeta"),
        p("C99")
     ) )
     
complex(p(HGNC:APP), p(HGNC:SORL1))

# die complexAbundances werden nicht als Enzyme identifiziert! explizit deklarieren!
complex(proteinAbundance(MSO:"Alpha secretase"), proteinAbundance(MSO:"Alpha secretase")) isA abundance(HUPSON:"allosteric enzyme")
complex(proteinAbundance(MSO:"Beta secretase"), proteinAbundance(MSO:"Beta secretase")) isA abundance(HUPSON:"allosteric enzyme")
complex(p(HGNC:APP), p(HGNC:APP))

p("sappalpha")
#irgendwie muss ich aber sagen,dass die activity enhanced wird (kann auch impaired werden bei allosterischen Enzymen!)
#Martin fragen ob das stimmt; ist noch nicht implementiert!!
#complex(proteinAbundance(MSO:"Alpha secretase"), proteinAbundance(MSO:"Alpha secretase")) -> catalyticActivity( complex(
#              complex(p(HGNC:APP), p(HGNC:APP)), 
#              complex(proteinAbundance(MSO:"Alpha secretase"), proteinAbundance(MSO:"Alpha secretase")))  )
p("C83")
p("C83_d")
p("sappalpha_d")
p("sappbeta_d")
        
reaction(
   reactants (
     complex(
       complex(p(HGNC:APP), p(HGNC:APP)), 
       complex(proteinAbundance(MSO:"Alpha secretase"), proteinAbundance(MSO:"Alpha secretase"))) 
     ),
   products (
        complex(proteinAbundance(MSO:"Alpha secretase"), proteinAbundance(MSO:"Alpha secretase")),
        p("sappalpha_d"),
        p("C83_d")
     ) )

p("sappbeta")
#irgendwie muss ich aber sagen,dass die activity enhanced wird (kann auch impaired werden bei allosterischen Enzymen!)
#complex(proteinAbundance(MSO:"Beta secretase"), proteinAbundance(MSO:"Beta secretase")) -> catalyticActivity(complex(
#              complex(p(HGNC:APP), p(HGNC:APP)), 
#              complex(proteinAbundance(MSO:"Beta secretase"), proteinAbundance(MSO:"Beta secretase"))))
p("C99")
p("C99_d")
        
reaction(
   reactants (
     complex(
       complex(p(HGNC:APP), p(HGNC:APP)), 
       complex(proteinAbundance(MSO:"Beta secretase"), proteinAbundance(MSO:"Beta secretase"))) 
       ),
   products (
        complex(proteinAbundance(MSO:"Beta secretase"), proteinAbundance(MSO:"Beta secretase")),
        p("sappbeta_d"),
        p("C99_d")
     ) )

UNSET Citation
UNSET STATEMENT_GROUP

